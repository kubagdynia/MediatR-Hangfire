{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  },
  "AllowedHosts": "*",
  "DatabaseOptions": {
    "ConnectionString": "Data Source=sqlite.db",
    "CommandTimeout": 10,
    "EnableSensitiveDataLogging": true,
    "EnableDetailedErrors": true
  }
}